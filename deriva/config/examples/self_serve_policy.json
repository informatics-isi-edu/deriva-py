  "group_list_table": {
    "schema": "_acl_admin",
    "table": "group_lists"
  },

  "acl_definitions": {
    "read_only": {
      "select": "users",
      "enumerate": "users",
      "insert": "empty",
      "update": "empty",
      "delete": "empty"
    },
    "self_serve": {
      "select": "users",
      "enumerate": "public",
      "insert": "writers_and_curators",
      "update": "curators",
      "delete": "curators",
      "owner": "admins"
    },
    "curation_only": {
      "insert": "curators",
      "update": "curators"
    },
    "group_owner_fk_acl": {
      "insert": "curators",
      "update": "curators"
    }
  },

  "acl_bindings": {
    "row_owner_guard": {
      "types": ["update", "delete"],
      "projection": ["RCB"],
      "projection_type": "acl"
    },
    "group_owner_guard": {
      "types": ["update", "delete"],
      "projection": ["Owner"],
      "projection_type": "acl"
    },
    "group_owner_fk_guard": {
      "types": ["update", "insert"],
      "projection": ["ID"],
      "projection_type": "acl"
    }
  },

  "catalog_acl": {
    "acl": "self_serve"
  },

